.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}
.topPadding{
  width: 100%;
  height: 88rpx;
}
.page{
  width: 100%;
  position: fixed;
  top: 0rpx;
  left: 0rpx;
}
/*  */
.search_bar{
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgb(255, 255, 255);
}
.search_bar_label{
  width: 710rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #9b9b9b;
  font-size: 28rpx;
  background-color: rgb(241, 241, 241);
  border-radius: 10rpx;
}
.icon_search{
  width:30rpx;
  height:30rpx;
  padding: 0 20rpx;
}
.search_bar_text{
  /* font-size: 28rpx; */
}
.search_bar_label_2{
  width: 712rpx;
  height: 58rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  color: #9b9b9b;
  font-size: 28rpx;
}
.search_bar_label_2_left{
  width: 622rpx;
  height: 56rpx;
  display: flex;
  /* justify-content: center; */
  align-items: center;
  border-radius: 10rpx;
  border: 1rpx rgb(241, 241, 241) solid;
}
.search_bar_label_2_left > input{
  width: 480rpx;
}
.clear_icon{
  width: 68rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.search_bar_label_2_left_right{
  margin-left:10px;
  line-height:28px;
  color:#09bb07;
  white-space:nowrap;
}
