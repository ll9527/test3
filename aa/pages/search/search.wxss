.body1{
  display: flex;
}
.body1_left{
  /* flex: 0 0 40px; */
  width: 192rpx;
  background-color: rgb(245, 245, 245);
}
.category{
  height: 108rpx;
  text-align: center;
  line-height: 108rpx;
  font-size: 28rpx;
  color: rgb(31, 31, 31);
}
.on{ 
  color: rgb(255, 90, 90); 
  background-color: rgb(255, 255, 255);
} 
.body1_right{
  width: 558rpx;  
  padding: 0 18rpx;
}
.body1_right_box{
  /* height: 710rpx; */
  padding-top: 40rpx;
}
.body1_right_box_top {
  height: 32rpx;
  line-height: 30rpx;
}
.body1_right_box_top > text{
  font-size: 28rpx;
  color: rgb(31, 31, 31);
}
.body1_right_box_top > navigator{
  position: relative;
  left: 390rpx;
  bottom: 30rpx;
  font-size: 28rpx;
  color: rgb(128, 128, 128);
}
.body1_right_box_bottom{
  /* width: 522rpx; */
  overflow: hidden;
}
.body1_right_box_bottom image{
  width: 168rpx;
  height: 168rpx;  
}
.body1_right_box_bottom_list{
  width: 168rpx;
  height: 210rpx;  
  margin-top: 26rpx;
  margin-right: 8rpx; 
  display: inline-block;
}
.body1_right_box_bottom_list > view{
  text-align: center;
  line-height: 28rpx;
  font-size: 24rpx;
  /* vertical-align:top; */
  color: rgb(31, 31, 31);
}


