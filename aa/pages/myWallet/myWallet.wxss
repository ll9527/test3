page{
  background-color: rgb(245, 245, 245);
  width: 710rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.card{
  height: 230rpx;
  padding: 40rpx;
  border-radius: 8rpx;
  color: rgb(255, 255, 255);
  background: linear-gradient(to bottom right, rgb(255,168,0) , rgb(251,198,72));
}
.card_title{
  width: 100%;
  height: 50rpx;
  line-height: 30rpx;
  font-size: 28rpx;
  display: flex;
  justify-content: space-between;
}
.toCash{
  padding-left: 30rpx;
  padding-bottom: 22rpx;
}
.text{
  font-size: 72rpx;
  display: flex;
  justify-content: center;
  padding-top: 30rpx;
}
.text_1{
  /* line-height: 72rpx; */
  padding-top: 50rpx; 
  padding-left: 10rpx; 
  font-size: 24rpx;
}
