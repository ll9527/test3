page{
  width: 670rpx;
  margin: 0 40rpx;
}
.inputBox{
  padding-left: 20rpx;
  height: 60rpx;
  border-radius: 8rpx;
  border: 1rpx rgb(238 , 238, 238) solid;
}
.text{
  color: rgb(46, 46, 46);
  font-size: 26rpx;
  padding-left: 20rpx;
  margin-bottom: 20rpx;
}
.button{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: rgb(233, 102, 102);
  border-radius: 8rpx;
  color: rgb(255, 255, 255);
  text-align: center;
  margin-bottom: 50rpx;
}
.picker{
  line-height: 60rpx;
}